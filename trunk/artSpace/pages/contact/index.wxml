<view class="loading box_s center {{__jq.loading.class}}" id="loading" style=" {{__jq.loading.style}}" data="{{__jq.loading.data}}" animation="{{__jq.loading.animation}}" bindtap="{{__jq.loading.tap}}"><image  class="loading_logo" src="../../image/logo.png" mode="aspectFit" />
  <view class="loading_text">AURA SPACE</view>
  <view class="loading_text1">THE WORLD OF ART</view>
  <view class="loading_pre">{{loading_pre}}</view>
  <view wx:for="{{loading_imgs}}" wx:key="index" style="width:100%;height:0;overflow:hidden;"><image  src="{{item}}" bindload="loadingImgLoaded" binderror="loadingImgLoaded" /></view>
</view>
<view class="top box_h">
  <view class="top_home center {{__jq.home_btn.class}}" id="home_btn" style=" {{__jq.home_btn.style}}" data="{{__jq.home_btn.data}}" animation="{{__jq.home_btn.animation}}" bindtap="{{__jq.home_btn.tap}}"><image  class="hover_animate" src="../../image/logo.png" mode="aspectFit" /></view>
  <view class="boxflex1"></view>
  <view class="top_menu center {{__jq.menu_btn.class}}" id="menu_btn" style=" {{__jq.menu_btn.style}}" data="{{__jq.menu_btn.data}}" animation="{{__jq.menu_btn.animation}}" bindtap="{{__jq.menu_btn.tap}}"><image  class="hover_animate" src="../../image/menu.png" mode="aspectFit" /></view>
</view>
<view class="menu {{__jq.menu.class}}" id="menu" style=" {{__jq.menu.style}}" data="{{__jq.menu.data}}" animation="{{__jq.menu.animation}}" bindtap="{{__jq.menu.tap}}">
  <view class="menu_body {{__jq.menu_body.class}}" id="menu_body" style=" {{__jq.menu_body.style}}" data="{{__jq.menu_body.data}}" animation="{{__jq.menu_body.animation}}" bindtap="{{__jq.menu_body.tap}}"><image  class="menu_close hove_animate" mode="aspectFit" src="../../image/close.png" />
    <view class="menu_main">
      <!--当前页面显示加 class='select'-->
      <view class="menu_item box_h" data-url="{{list.url}}" wx:for="{{menu}}" wx:key="index1" wx:for-item="list">
        <view class="{{ (text ==' ') ? 'space' : '' }}" wx:for="{{list.name}}" wx:key="index2" wx:for-item="text">{{text}}</view>
      </view>
    </view>
  </view>
</view>
