.breakall{
  word-wrap:break-word;
  word-break:break-all;
}

.diandian{
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow:hidden;
}
.diandian2{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.diandian3{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}
.hover_animate{
    -webkit-transition: all 0.2s linear;
    transition: all 0.2s linear;
}